<h2>My Lists</h2>
<div class="cards">
  <% @lists.each do |list| %>
  <% img = list.photo.attached? ? list.photo.key : "defaultmovielist" %>
    <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path img, height: 300, crop: :fill %>')">
      <%= link_to list.name.capitalize, list_path(list) %>
    </div>
  <% end %>
</div>
